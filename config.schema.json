{
  "pluginAlias": "Thermostat",
  "pluginType": "platform",
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name of the device",
        "type": "string",
        "required": true,
        "default": "Homebridge Inkbird Wifi Gateway Dynamic Platform"
      },
      "model": {
        "type": "string",
        "enum": [
          "IBS-MS1",
          "not in list - try it anyway"
        ],
        "title": "Model number (i.e. IBS-MS1, leave empty if not sure)"
      },
      "host": {
        "title": "IP Address / Hostname",
        "type": "string",
        "required": true,
        "format": "hostname"
      }
    }
  }
}